<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Producto.
    -->
    <changeSet id="20220201093943-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="iva_id"
                                 baseTableName="producto"
                                 constraintName="fk_producto__iva_id"
                                 referencedColumnNames="id"
                                 referencedTableName="iva"/>

        <addForeignKeyConstraint baseColumnNames="empresa_id"
                                 baseTableName="producto"
                                 constraintName="fk_producto__empresa_id"
                                 referencedColumnNames="id"
                                 referencedTableName="empresa"/>

        <addForeignKeyConstraint baseColumnNames="pedido_id"
                                 baseTableName="producto"
                                 constraintName="fk_producto__pedido_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pedido"/>

        <addForeignKeyConstraint baseColumnNames="almacen_id"
                                 baseTableName="producto"
                                 constraintName="fk_producto__almacen_id"
                                 referencedColumnNames="id"
                                 referencedTableName="almacen"/>
    </changeSet>
</databaseChangeLog>
